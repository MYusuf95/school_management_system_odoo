 <odoo>
  <data>

      <record model="ir.ui.view" id="school_management.attendance_view">
      <field name="name">school_management list</field>
      <field name="model">school_management.attendance</field>
      <field name="arch" type="xml">
        <tree  editable="bottom" >
              <field name="student_id"/>
              <!-- add classes field from timetable module here -->
              <field name="attendance"/>
        </tree>
      </field>
    </record>



    <record model="ir.ui.view" id="school_management.attendance_form_view">
      <field name="name">school_management list</field>
      <field name="model">school_management.attendance</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>


              <field name="attendance"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

  </data>
</odoo>